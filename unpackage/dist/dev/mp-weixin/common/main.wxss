/* 卡片 */
.card {
  width: 654rpx;
  padding-bottom: 30rpx;
  background: white;
  border-radius: 40rpx;
  overflow: hidden;
  box-shadow: rgba(0, 0, 0, 0.03) 1px 2px 10px 5px;
  margin-bottom: 50rpx;
}
.card .card_img_wrapper {
    position: relative;
}
.card .card_img_wrapper .card_img {
      width: 100%;
      height: 466rpx;
}
.card .card_img_wrapper .hot {
      position: absolute;
      top: 42rpx;
      left: 36rpx;
      color: #FFF;
      background: #025392;
      border-radius: 22rpx;
      padding: 10rpx 24rpx;
}
.card .card_img_wrapper .living {
      position: absolute;
      top: 42rpx;
      right: 36rpx;
      color: #FFF;
      background: #025392;
      border-radius: 22rpx;
      padding: 10rpx 24rpx;
}
.card .card_dec_wrapper {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    margin: 38rpx 32rpx 0;
    line-height: 36rpx;
    text-align: left;
}
.card .card_dec_wrapper .title {
      font-size: 1.6em;
      font-weight: 700;
      margin-bottom: 22rpx;
      color: #333333;
      line-height: 1.3;
}
.card .card_dec_wrapper .des {
      font-size: 1em;
      line-height: 1.5em;
      color: #808080;
      display: -webkit-box;
      -webkit-box-orient: vertical;
      -webkit-line-clamp: 1;
      text-overflow: ellipsis;
      overflow: hidden;
      max-width: 500rpx;
}
.card .card_dec_wrapper .listBottom {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
}
.card .card_dec_wrapper .price {
      margin-top: -12rpx;
      color: #FD8E23;
}
.card .card_dec_wrapper .price text {
        font-size: 36rpx;
}
._h2.detailH2 {
  text-align: left;
  font-size: 36rpx;
  font-weight: 700;
  margin: 80rpx 0 40rpx 0;
}
.headerTitle {
  position: absolute;
  top: 0rpx;
  top: 25px;
  line-height: 25px;
  left: 30px;
  z-index: 2;
  font-size: 30rpx;
  z-index: 222;
}
.wrap {
  margin: 0 48rpx;
}

