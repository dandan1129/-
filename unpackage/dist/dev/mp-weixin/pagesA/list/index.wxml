ail':'filter_detail']}}"><block wx:for="{{filterItem[filter]}}" wx:for-item="item" wx:for-index="index" wx:key="value"><view data-event-opts="{{[['tap',[['hideDialog',[true,index,'$0'],[[['filterItem.'+filter+'','value',item.value,'value']]]]]]]}}" class="{{[item.active?'filter_line activeText':'filter_line']}}" catchtap="__e"><text>{{item.title}}</text></view></block></view></view></view><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['currentTap',['$0'],[[['listData','id',item.id,'id']]]]]]]}}" class="pull" bindtap="__e"><view class="{{['card skeleton-rect lazy-'+index]}}"><view class="card_img_wrapper"><view class="card_img"><lazy-img vue-id="{{('4-'+index)+','+('2')}}" imgSrc="{{item.isShow?item.atts:''}}" bind:__l="__l"></lazy-img></view></view><view class="card_dec_wrapper"><text class="title">{{item.title}}</text><text class="des">{{item.content}}</text><view class="listBottom"><star vue-id="{{('5-'+index)+','+('2')}}" score="{{item.score}}" bind:__l="__l"></star><view class="price">¥<text>60</text>/人</view></view></view></view></view></block></mescroll></view>